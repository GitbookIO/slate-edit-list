{
  "parser": "babel-eslint",
  "extends": ["gitbook"],
  "rules": {
    "no-param-reassign": 0,
    "import/no-commonjs": 2,
    "react/void-dom-elements-no-children": 0,
    "jsx-a11y/html-has-lang": 0,
    "jsx-a11y/accessible-emoji": 0
  },
  "plugins": ["flowtype"],
  "env": {
    "mocha": true
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
